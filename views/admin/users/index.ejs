<%- include("../../partials/header.ejs") %>
    <%- include("../../partials/navbar.ejs") %>

        <div class="container">
            <br>
            <h2>Lista de usuarios</h2>
            <a href="/admin/users/create" class="btn btn-success mt-2 mb-5">Cadastrar novo usu√°rios</a>


            <table class="table table-bordered table-dark table-striped">
                <thead>
                    <tr>
                        <th>ID</th>
                        <th>Nome</th>
                        <th>Email</th>
                    </tr>
                </thead>
                <tbody>
                    <% users.forEach(users=>{ %>
                        <tr>
                            <th><%= users.id %></th>
                            <th><%= users.name %></th>
                            <th><%= users.email %></th>
                        </tr>
                        <% }) %>
                </tbody>
            </table>

        </div>



        <%- include("../../partials/footer.ejs") %>